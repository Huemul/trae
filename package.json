{
  "name": "trae",
  "version": "0.0.10",
  "description": "Fetch based HTTP client for the browser",
  "keywords": [
    "fetch",
    "http",
    "request",
    "api"
  ],
  "main": "dist/trae.min.js",
  "author": "gillchristian <gillchristiang@gmail.com>",
  "contributors": [
    "ndelvalle <nicolas.delvalle@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huemul/trae.git"
  },
  "bugs": {
    "url": "https://github.com/huemul/trae/issues"
  },
  "homepage": "https://github.com/huemul/trae#readme",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:dev": "rollup -c",
    "build:dev:w": "rollup -c -w",
    "test": "npm run eslint && jest --coverage --verbose",
    "test:w": "jest --watch --verbose",
    "check-cover": "open coverage/lcov-report/index.html",
    "check-bundle": "open coverage/bundle-statistics.html",
    "eslint": "eslint . --ext .js",
    "prepublish": "npm run build",
    "release": "np"
  },
  "dependencies": {
    "merge": "^1.2.0",
    "np": "^2.11.0",
    "trae-query": "^0.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "fetch-mock": "^5.5.0",
    "jest": "^16.0.2",
    "node-inspector": "^0.12.8",
    "pre-commit": "^1.1.3",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-conditional": "^0.1.4",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-visualizer": "^0.1.5",
    "rollup-watch": "^2.5.0"
  },
  "pre-commit": [
    "eslint",
    "test"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "lib/**"
    ],
    "testPathDirs": [
      "test/"
    ]
  },
  "engines": {
    "node": ">=6"
  }
}
