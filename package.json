{
  "name": "trae",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "main": "src/index.js",
    "test": "jest --coverage",
    "test:w": "jest --watch --verbose",
    "check-cover": "open coverage/lcov-report/index.html",
    "eslint": "eslint . --ext .js"
  },
  "devDependencies": {
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "fetch-mock": "^5.5.0",
    "jest": "^16.0.2",
    "pre-commit": "^1.1.3"
  },
  "pre-commit": [
    "eslint"
  ],
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "src/**"
    ],
    "testPathDirs": [
      "test/"
    ]
  },
  "dependencies": {
    "trae-query": "^0.1.0",
    "whatwg-fetch": "^1.0.0"
  }
}
