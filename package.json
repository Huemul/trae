{
  "name": "trae",
  "version": "1.4.2",
  "description": "Fetch based HTTP client for the browser",
  "keywords": [
    "fetch",
    "http",
    "request",
    "api"
  ],
  "main": "dist/trae.js",
  "module": "dist/trae.umd.min.js",
  "unpkg": "dist/trae.umd.min.js",
  "files": [
    "src",
    "dist/*.js"
  ],
  "contributors": [
    "gillchristian <gillchristiang@gmail.com>",
    "ndelvalle <nicolas.delvalle@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huemul/trae.git"
  },
  "bugs": {
    "url": "https://github.com/huemul/trae/issues"
  },
  "homepage": "https://github.com/huemul/trae#readme",
  "scripts": {
    "test:docs": "chimi",
    "add": "all-contributors add",
    "contributors": "all-contributors generate",
    "build": "NODE_ENV=production node build/rollup.js",
    "build:dev": "node build/rollup.js",
    "docs": "http-server ./docs -o",
    "test": "npm run eslint && jest --coverage --verbose",
    "test:w": "jest --watch --verbose",
    "check-cover": "open coverage/lcov-report/index.html",
    "check-bundle": "open coverage/bundle-statistics.html",
    "ts:check": "tsc --pretty --noEmit",
    "ts:check:watch": "tsc --pretty --noEmit --watch",
    "prepublish": "in-publish && ./scripts/publish.sh || echo 'just installing'",
    "eslint": "eslint . --ext .js",
    "release": "np"
  },
  "dependencies": {
    "@types/jest": "^25.1.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^25.1.0",
    "lodash": "^4.17.15",
    "merge": "^1.2.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "qs": "^6.9.1",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "all-contributors-cli": "^6.13.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chimi": "^0.2.2",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "fetch-mock": "^8.3.2",
    "http-server": "^0.12.1",
    "in-publish": "^2.0.0",
    "jest": "^25.1.0",
    "mkdirp": "^1.0.3",
    "np": "^5.2.1",
    "pre-commit": "^1.2.2",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-conditional": "^3.1.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^3.3.1",
    "rollup-watch": "^4.3.1"
  },
  "pre-commit": [
    "eslint",
    "test"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "lib/**"
    ],
    "roots": [
      "test/"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  },
  "engines": {
    "node": ">=6"
  }
}
